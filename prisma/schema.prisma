
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Student {
  id             String  @id @default(auto()) @map("_id") @db.ObjectId // âœ… Correct MongoDB ID
  registrationNo String  @unique
  name           String
  class          String
  rollNo         Int
  contactNumber  String
  status         Boolean @default(true)

  @@unique([class, rollNo])
}
